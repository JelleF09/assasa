    Start-Sleep -Seconds 1;
    $sig = '[DllImport(\"user32.dll\")] public static extern bool MoveWindow(IntPtr hWnd, int X, int Y, int nWidth, int nHeight, bool bRepaint);';
    add-type -name Win32 -member $sig -namespace Funcs;
    $hwnd = (Get-Process Windows64 | Where-Object { $_.MainWindowTitle -ne '' }).MainWindowHandle;
